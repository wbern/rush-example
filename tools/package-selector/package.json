{
    "name": "package-selector",
    "version": "1.0.0",
    "description": "An example toolchain used to build projects in this repo",
    "license": "MIT",
    "scripts": {
        "build": "echo hello",
        "build-with-dependencies": "rush build --to package-selector --to package-selector",
        "start": "node index.js"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "parserOptions": {
            "parser": "babel-eslint",
            "ecmaVersion": 2019
        },
        "extends": [
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "off",
            "no-case-declarations": "off"
        },
        "overrides": [
            {
                "files": [
                    "**/__tests__/*.{j,t}s?(x)"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "dependencies": {
        "ansi-escapes": "^3.0.0",
        "ansi-styles": "^3.2.1",
        "fuzzy": "^0.1.3",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.7.2",
        "rxjs": "^5.5.2",
        "run-async": "^2.3.0",
        "node-emoji": "1.10.0",
        "inquirer": "^5.2.0",
        "inquirer-autocomplete-prompt": "^0.12.2",
        "strip-ansi": "^4.0.0",
        "@microsoft/rush-lib": "5.20.0"
    },
    "devDependencies": {}
}
